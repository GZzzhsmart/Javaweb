<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="testDAO" class="com.gs.dao.impl.TestDAOImpl" autowire="byName"></bean>
	<bean id="BaseDAO" class="com.gs.dao.impl.loginDAOImpl" autowire="byName"></bean>
	<bean id="testService" class="com.gs.service.impl.TestServiceImpl" autowire="byName"></bean>
	<bean id="loginService" class="com.gs.service.impl.loginServiceImpl" autowire="byName"></bean>
	<bean id="testAction" class="com.gs.controller.TestAction" autowire="byName" scope="prototype"></bean>
	<bean id="loginAction" class="com.gs.controller.loginAction" autowire="byName" scope="prototype"></bean>
</beans>